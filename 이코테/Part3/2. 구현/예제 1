from collections import deque
import sys
import heapq

N = sys.stdin.readline().strip()

length = len(N)
sum1 = 0
sum2 = 0

for i in range(0, length // 2):
    sum1 += int(N[i])
for i in range(length // 2, length):
    sum2 += int(N[i])
if sum1 == sum2 :
    print("LUCKY")
else :
    print("READY")


